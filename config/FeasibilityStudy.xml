<!-- OutputLevel controls which messages are printed; set to VERBOSE or DEBUG for more verbosity, to WARNING or ERROR for less -->
<JobConfiguration JobName="FeasibilityStudyJob" OutputLevel="INFO">
  <Library Name="libSUHH2BoostedHiggsToWW"/>
  <Package Name="SUHH2BoostedHiggsToWW.par" />

  <Cycle Name="uhh2::AnalysisModuleRunner" OutputDirectory="&OUTDIR;/" PostFix="" TargetLumi="1" >

    <!-- MC -->
    <InputData Type="MC"    Version="MC_DY1JetsToLL"            Lumi="37342.76571"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_DY1JetsToLL;        <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_DY2JetsToLL"            Lumi="38016.12332"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_DY2JetsToLL;        <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_DY3JetsToLL"            Lumi="39815.75251"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_DY3JetsToLL;        <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_DY4JetsToLL"            Lumi="106999.6291"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_DY4JetsToLL;        <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_TTbarSemiLeptonic"      Lumi="91335604.46"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_TTbarSemiLeptonic;  <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_TTTo2L2Nu"              Lumi="56758143.39"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_TTTo2L2Nu;          <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_TTToHadronic"           Lumi="100683950.1"  NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_TTToHadronic;       <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>

    <InputData Type="MC"    Version="MC_WZ"                     Lumi="21005.792"    NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_WZ;                 <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>
    <InputData Type="MC"    Version="MC_ZZ"                     Lumi="60107.365"    NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_ZZ;                 <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>

    <InputData Type="MC"    Version="MC_HZ_HiggsToWWZToLL"      Lumi="365384615.38" NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &MC_HZ_HiggsToWWZToLL;  <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>

    <!-- Data -->
    <InputData Type="DATA"  Version="Data_2017C"                Lumi="1"            NEventsMax="&NEVT;" Cacheable="&CACHEABLE;">  &Data_2017C;            <InputTree Name="AnalysisTree"/> <OutputTree Name="AnalysisTree"/> </InputData>


    <UserConfig>
      <!-- define which collections to read from the input. Only specify what you need to save I/O time -->
      <Item Name="PrimaryVertexCollection"    Value="offlineSlimmedPrimaryVertices" />
      <Item Name="ElectronCollection"         Value="slimmedElectronsUSER" />
      <Item Name="MuonCollection"             Value="slimmedMuonsUSER" />
      <Item Name="TauCollection"              Value="slimmedTaus" />
      <Item Name="JetCollection"              Value="slimmedJets" /> <!-- slimmedJetsAK8_SoftDrop, slimmedJets -->
      <Item Name="METName"                    Value="slimmedMETs" />
      <Item Name="GenInfoName"                Value="genInfo" />
			<Item Name="GenParticleCollection"      Value="GenParticles" />
      <Item Name="GenJetCollection"           Value="slimmedGenJetsAK8" />
      <Item Name="TopJetCollection"           Value="packedPatJetsAk8CHSJets_SoftDropCHS" />  <!-- slimmedJetsAK8_SoftDrop -->
      <!-- The list of available collections to read in corresponds to the pointer members in event. The 'Name's to use here in the configuration are:

      PrimaryVertexCollection
      ElectronCollection
      MuonCollection
      TauCollection
      PhotonCollection
      JetCollection
      TopJetCollection
      METName

      GenInfoName
      GenJetCollection
      GenTopJetCollection
      GenParticleCollection

      Note that the last four are only read in for MC (InputData Type="MC"), so it is save to specify
      them for real data, although those branches do not exist.

      As 'Value', always use the branch name (if in doubt, open the input file in ROOT).

      By default, triggers are read. To explicitly disable reading triggers, use:
      <Item Name="readTrigger" Value="false" />
    -->
    <Item Name="readTrigger" Value="false" />
    <Item Name="trigger" Value="HLT_Mu17_TrkIsoVVL_Mu8_TrkIsoVVL_v*" />

    <!-- tell AnalysisModuleRunner NOT to use the MC event weight from SFrame; rather let
    MCLumiWeight (called via CommonModules) calculate the MC event weight. The MC
    event weight assigned by MCLumiWeight is InputData.Lumi / Cycle.TargetLumi. -->
    <Item Name="use_sframe_weight"          Value="false"/>

    <Item Name="lumi_file"                  Value="/nfs/dust/cms/user/amalara/WorkingArea/UHH2_94/CMSSW_9_4_1/src/UHH2/common/data/Cert_294927-306462_13TeV_EOY2017ReReco_Collisions17_JSON_v1_normTag.root"/>
    <Item Name="lumihists_lumi_per_bin"     Value="500."/> <!-- used in UHH2/common/include/LuminosityHists.h -->

    <!-- Systematic uncertainties -->
    <Item Name="jersmear_direction"         Value="&JERSMEAR_DIRECTION;"/>  <!--used only in UHH2/common/include/JetCorrections.cxx -->
    <Item Name="jecsmear_direction"         Value="&JECSMEAR_DIRECTION;"/>  <!--used only in UHH2/common/include/JetCorrections.cxx -->

    <!-- MC PuReweighting -->
    <Item Name="SysType_PU"                 Value="&SYSTYPE_PU;"/>
    <Item Name="pileup_directory"           Value="&PILEUP_DIRECTORY;"/>            <!--used only in UHH2/common/include/MCWeight.h -->
    <Item Name="pileup_directory_data"      Value="&PILEUP_DIRECTORY_DATA;"/>       <!--used only in UHH2/common/include/MCWeight.h -->
    <Item Name="pileup_directory_data_up"   Value="&PILEUP_DIRECTORY_DATA_UP;"/>    <!--used only in UHH2/common/include/MCWeight.h -->
    <Item Name="pileup_directory_data_down" Value="&PILEUP_DIRECTORY_DATA_DOWN;"/>  <!--used only in UHH2/common/include/MCWeight.h -->

    <!-- the class name of the AnalysisModule subclasses to run: -->
    <Item Name="AnalysisModule"             Value="FeasibilityStudyModule" />

  </UserConfig>
</Cycle>
</JobConfiguration>
